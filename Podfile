platform :ios, '10.0'

target 'facebook-twiter-search' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
pod 'FBSDKCoreKit'
pod 'FBSDKShareKit'
pod 'FBSDKLoginKit'
pod 'TwitterKit'

  target 'facebook-twiter-searchTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'facebook-twiter-searchUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
